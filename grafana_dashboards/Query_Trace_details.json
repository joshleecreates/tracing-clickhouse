          "formattedQuery": "SELECT $timeSeries as t, count() FROM $table WHERE $timeFilter GROUP BY t ORDER BY t",
          "interval": "",
          "intervalFactor": 1,
          "query": "WITH \r\n  (SELECT min(Start) FROM \"default\".\"otel_traces_trace_id_ts\"\r\n    WHERE query_id = '$queryId') as trace_start,\r\n  (SELECT max(End) + 1 FROM \"default\".\"otel_traces_trace_id_ts\"\r\n    WHERE query_id = '$queryId') as trace_end\r\nSELECT \"TraceId\" as traceID,\r\n  \"SpanId\" as spanID,\r\n  \"ParentSpanId\" as parentSpanID,\r\n  \"ServiceName\" as serviceName,\r\n  \"SpanName\" as operationName,\r\n  \"Timestamp\" as startTime,\r\n  \"Duration\" as duration,\r\n  arrayMap(key -> map('key', key, 'value',\"SpanAttributes\"[key]),\r\n  mapKeys(\"SpanAttributes\")) as tags,\r\n  \"ResourceAttributes\" as serviceTags\r\nFROM \"default\".\"otel_traces\"\r\nWHERE TraceId IN (SELECT DISTINCT TraceId FROM otel_traces WHERE ResourceAttributes['clickhouse.initial_query_id'] = '$queryId'  OR ResourceAttributes['clickhouse.query_id'] = '$queryId') --AND   `FinishDate` = toDate(trace_end)\r\n --AND startTime >= trace_start\r\n  --AND startTime <= trace_end AND `FinishDate` = toDate(trace_end)\r\n --AND startTime >= trace_start\r\n -- AND startTime <= trace_end\r\nORDER BY startTime\r\nLIMIT 1000",
          "rawQuery": "/* grafana dashboard=Query Trace Details, user=0 */\nWITH \r\n  (SELECT min(Start) FROM \"default\".\"otel_traces_trace_id_ts\"\r\n    WHERE query_id = '1aa99f66-79eb-4508-8bbe-7e43abb07c75') as trace_start,\r\n  (SELECT max(End) + 1 FROM \"default\".\"otel_traces_trace_id_ts\"\r\n    WHERE query_id = '1aa99f66-79eb-4508-8bbe-7e43abb07c75') as trace_end\r\nSELECT \"TraceId\" as traceID,\r\n  \"SpanId\" as spanID,\r\n  \"ParentSpanId\" as parentSpanID,\r\n  \"ServiceName\" as serviceName,\r\n  \"SpanName\" as operationName,\r\n  \"Timestamp\" as startTime,\r\n  \"Duration\" as duration,\r\n  arrayMap(key -> map('key', key, 'value',\"SpanAttributes\"[key]),\r\n  mapKeys(\"SpanAttributes\")) as tags,\r\n  \"ResourceAttributes\" as serviceTags\r\nFROM \"default\".\"otel_traces\"\r\nWHERE TraceId IN (SELECT DISTINCT TraceId FROM otel_traces WHERE ResourceAttributes['clickhouse.initial_query_id'] = '1aa99f66-79eb-4508-8bbe-7e43abb07c75'  OR ResourceAttributes['clickhouse.query_id'] = '1aa99f66-79eb-4508-8bbe-7e43abb07c75') \n \n  -AND startTime <= trace_end AND `FinishDate` = toDate(trace_end)\r\n \n \nORDER BY startTime\r\nLIMIT 1000",
