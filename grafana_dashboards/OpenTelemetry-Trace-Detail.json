      "title": "Trace Summary",
      "type": "table",
      "targets": [
        {
          "datasource": {
            "type": "vertamedia-clickhouse-datasource",
            "uid": "${datasource}"
          },
          "format": "table",
          "query": "SELECT\n  TraceId,\n  min(Timestamp) as StartTime,\n  max(Timestamp + Duration / 1000000) as EndTime,\n  max(Timestamp + Duration / 1000000) - min(Timestamp) as TotalDuration,\n  count() as SpanCount\nFROM default.otel_traces\nWHERE TraceId = '${traceId}'\nGROUP BY TraceId",
          "rawQuery": true,
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {
        "type": "vertamedia-clickhouse-datasource",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
