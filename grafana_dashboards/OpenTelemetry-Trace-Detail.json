      "type": "table",
      "targets": [
        {
          "datasource": {
            "type": "vertamedia-clickhouse-datasource",
            "uid": "${datasource}"
          },
          "format": "table",
          "query": "SELECT\n  SpanId,\n  ParentSpanId,\n  ServiceName,\n  SpanName,\n  toDateTime(Timestamp) as Timestamp,\n  Duration\nFROM default.otel_traces\nWHERE TraceId = '${traceId}'\nORDER BY Timestamp\nLIMIT 1000",
          "rawQuery": true,
          "refId": "A"
        }
      ]
    }
